<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
    namespace：必须与对应的接口全类名一致  UserMapper.java
    id ：必须与对应接口的某个对应的方法名一致即必须要和UserMapper.java接口中的方法同名。
 -->
<mapper namespace="com.zafu.nichang.web.mapper.ProductMapper">

    <insert id="insertProduct" parameterType="Product">
        insert into product_everyday_detail(product_name,min_price,avg_price,max_price,size_type,unit,date_time,product_type)
        values(#{productName},#{min_price},#{avgPrice},#{maxPrice},#{sizeType},#{unit},#{dateTime},#{threadNumber})
    </insert>

</mapper>